   <extension
         point="org.eclipse.osee.framework.ui.plugin.XNavigateItemProvider">
      <XNavigateItemProvider
            classname="org.eclipse.ote.ui.message.search.internal.MessageNavigateViewItems">
      </XNavigateItemProvider>
   </extension>
</plugin>
